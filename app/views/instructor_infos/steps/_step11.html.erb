        <a class="modify-link step-navigate initialize-section" data-target="#step5" data-section="" data-percentage='75%'><%= I18n.t("registration.step11.modify") %></a>
      </div>
      <div class="col-md-2 tick-success">
        <%= image_tag "registration/tick-success.png", class: 'img-responsive'%>
