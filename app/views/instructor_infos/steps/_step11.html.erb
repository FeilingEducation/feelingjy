      </div>
      <div class="col-md-2 tick-success">
        <%= image_tag "registration/tick-success.png", class: 'img-responsive'%>
      </div>
    </div>
    <hr/>
    <div class="row">
      <div class="col-md-12 disable-block">
        <div class="head-sec">
          <h6 class="sub-heading"><%= I18n.t("registration.step11.heading3") %></h6>
        </div>
      </div>
      <div class="col-md-12">
        <p class="sub-heading"><%= I18n.t("registration.step11.desc3") %></p>
        <button type="button" class="btn btn-primary step-navigate main-btn pull-left initialize-section" data-section="sec3" data-target="#step12" data-percentage="1%"><%= I18n.t("registration.step11.continue") %></button>
      </div>
    </div>
