/*profile CSS*/
.profile-main {
	border-top: 3px solid #5ad3b1;
	background: #fff;
	margin: 60px 0;
	padding: 0 0 60px 0;
}

.profile-main > .header {
	text-align: center;
	color: #979999;
	// padding-bottom: 30px;
	padding: 60px 0 30px 0;
	position: relative;

	.editable{
		border: 0!important;

		&.background{
			position: absolute;
			text-align: right;
			cursor: pointer;
			top: 30px;
			right: 30px;
			background: #fff;
			padding: 10px;
			background: #fafafa;
			border-radius: 100%;
			border: 0px;

			img{
				margin-top: -2px;
			}

			&.user-profile{
				bottom: 9px;
				top: auto;
				right: 0;
			}
		}
		&.edit-icon{
			position: absolute;
			right: 10px;
		}
	}

	.hatcher-center{
		min-width: 280px;
		margin: 30px auto;
		background: rgba(254, 254, 254,.8);
		padding: 10px;
		border-radius: 10px;
		max-width: 35px;
	}
}

.profile-main .header .img {
	position: relative;
	border-radius: 100%;
	height: 180px;
	width: 180px;
	background-size: cover;
	background-position: center;
	margin: 0px auto;
}

.profile-main .header .img .camera {
	position: absolute;
	height: 42px;
	width: 42px;
	background: #fafafa;
	border-radius: 100%;
	bottom: 0;
	right: 0;
}

.profile-main .header .img .camera img {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

.profile-main .header .user-name {
	color: $theme-green;
	font-size: 28px;
	font-weight: normal;
	margin: 20px 0 25px;
}

.profile-main  .profile-summary {
	background: #fafafa;
	padding: 20px 45px;
	position: relative;
	color: #979999;
	text-align: left;
	line-height: 26px;

	&:first-child{
		padding-bottom: 0px;
	}
}

.profile-main  .profile-summary .edit-icon {
	position: absolute;
	top: 6px;
