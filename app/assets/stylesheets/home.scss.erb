@import "halvetica-rounded";

// $main-text-color: #717070;
$main-text-color: #979999;
$question-text: #4A4A4A;
$theme-green: #31c89d;
$border-color: #E0DBDB;

body {
	padding-top: 0;
	// background: #fff;
	// font-family: 'helvatica-r-bd';
	font-family: 'Roboto-Light', 'Tahoma','sans-serif';
	line-height: normal;
	font-size: 14px;
	// background-color: #e6e6e6;
}
h1,h2,h3,h4,h5,h6{
	font-family: 'Tahoma','sans-serif';
}

.main-content{
	// background: #e6e6e6;
	min-height: 80vh;
	height: auto;

	&.dark{
		// background: #e6e6e6;
		background: #f9f9f9;
	}
}
.personal-center{
	// background: #e6e6e6;
}

.form-control, .btn {
	font-family: 'helvatica-r-bd';
	// font-family: 'Roboto', sans-serif;

	font-size: 16px;
	outline: none !important;
}

.form-control:focus {
	color: #979999;
	border: 1px solid rgba(0, 0, 0, 0.15) !important;
}

.default-btn {
	background: $theme-green;
	color: #fff;
	border: 2px solid $theme-green;
	cursor: pointer;
	transition: all .4s ease;

	&:hover {
		color: $theme-green;
		background: transparent;
		cursor: pointer;
		transition: all .4s ease;
	}

	&.empty {
		color: $theme-green;
		background: transparent;
		cursor: pointer;
		transition: all .4s ease;
		&:hover {
			background: $theme-green;
			color: #fff;
		}
	}
	&.message-box{
		float: right;
	}
	&.red{
		background: #ff6666;
		color: #fff;
		border: 3px solid #ff6666;

		&:hover{
			color: #ff6666;
			background: none;
			border: 3px solid #ff6666;
		}
	}
}

img {
	max-width: 100%;
}

ul {
	padding: 0;
	margin: 0;
}

li {
	list-style: none;
}

a {
	text-decoration: none !important;
	transition: all .5s ease;
}

input {
	&[type="text"], &[type="password"], &[type="email"], &[type="number"] {
		margin-bottom: 0;
	}
}

select, button {
	margin-bottom: 0;
}

.top-bar {
	background: $theme-green;
	color: #fff;
	padding: 6px 0px 8px;
}

.phone-num img {
	margin-right: 8px;
}

.top-bar {
	a {
		color: #fff;
	}
	display: flex;
	justify-content: flex-end;
	ul, li {
		display: inline-block;
	}
	.dropdown .fa {
		font-size: 17px;
		font-weight: bold;
		margin-left: 5px;
	}
}

.dropdown.show .fa {
	transform: rotate(-180deg);
}

.top-bar .login {
	margin-left: 15px;
	li {
		&:first-child {
			// border-right: 2px solid #fff;
		}
		a {
			display: block;
			padding: 0 15px;
		}
	}
}

.dropdown-menu.top-bar-drop {
	background: #2dbd94;
	min-width: 160px;
	padding: 0px;
	font-size: 14px;
	transform: none !important;
	top: 27px !important;

	&.dropdown-item {
		padding: 6px 13px;
		color:#fff!important;

		a{
			color:#fff!important;
		}
		&:hover {
			background: #fff;
			color: #2dbd94;
		}
	}
}

.dropdowns li:first-child {
	margin-right: 20px;
}

.dropdown-menu.top-bar-drop a {
	img {
		margin-right: 5px;
		width: 22px;
		height: 16px;
	}
	&.dropdown-item {
		padding: 6px 13px;
		color: #fff!important;

		&:hover {
			background: #fff;
			color: #2dbd94!important;
		}
	}
}

.main-slider {
	position: relative;
	.search-form{
		position: absolute;
		// bottom: 80px;
		z-index: 999;
		width: 100%;
		top: 60%;
	}
	.slider-overlay {
		position: absolute;
		background: rgba(0, 0, 0, 0.4);
		height: 100%;
		width: 100%;
		top: 0;
		left: 0;
	}
	.carousel-caption {
		top: 40%;
		transform: translateY(-50%);
		bottom: auto;
		padding-top: 0;
		padding-bottom: 0;
		h1 {
			font-size: 50px;
			color: $theme-green;
			font-weight: normal;
			margin-bottom: 20px;
		}
		p {
			font-size: 30px;
			color: #fff;
		}
	}
	.search-con {
		// margin-top: 70px;
	}
}

.search-con {
	max-width: 735px;
	width: 100%;
	margin: 0px auto;
	background: #fff;
	padding: 3px 5px 5px 15px;
	border-radius: 40px;
	position: relative;
	text-align: left;
}

select.form-control {
	appearance: none;
	-webkit-appearance: none;
	color: #979999;
}

.select-con {
	position: relative;
	> {
		.fa {
			position: absolute;
			color: #979999;
			font-size: 24px;
			right: 15px;
			top: 8px;
		}
		select {
			padding-right: 35px;
		}
	}
}

.search-con {
	.select-con {
		display: inline-block;
		width: 40%;
		> .form-control {
			border: none !important;
		}
	}
	.btn {
		font-size: 18px;
		border-radius: 40px;
		padding: 6px 14px;
		position: absolute;
		right: 5px;
		top: 4px;
	}
}

.carousel-indicators li {
	width: 15px;
	height: 15px;
	border-radius: 100%;
	margin-right: 14px;
	cursor: pointer;
	background: #fff;
	&.active {
		width: 44px;
		border-radius: 14px;
		background: $theme-green;
	}
	&:last-child {
		margin-right: 0px;
	}
}

.section {
	padding: 40px 0px;
	// border-bottom: 2px solid #e7f7f2;
	&.grey-section {
		background: #f9f9f9;

		.mentors{
			.instructor-profile-box{
				margin-bottom: 30px;
			}
		}
	}
	.header {
		text-align: center;
		padding-bottom: 30px;

		h1 {
			font-size: 42px;
			color: $theme-green;
			position: relative;
			margin-bottom: 30px;
			// &:after {
			// 	content: "";
			// 	display: block;
			// 	position: absolute;
			// 	height: 3px;
			// 	width: 65px;
			// 	background: $theme-green;
			// 	left: 50%;
			// 	margin-left: -33px;
			// 	bottom: -35px;
			// }
		}
		p {
			margin-bottom: 0px;
			font-size: 16px;
			color: #979999;
		}
	}
}

.steps-con {
	h3 {
		font-size: 22px;
		color: $theme-green;
		margin: 20px 0 20px;
	}
	p {
		color: #979999;
		font-size: 16px;
	}
}

.div-bg {
	transition: all .5s ease;
	cursor: pointer;
	box-shadow: 2px 2px 20px #ccc;
	border-radius: 10px;

	.user-info{
		padding: 10px 10px 30px 10px;
	}

	.img {
		height: 300px;
		background-size: cover;
		background-position: center;
		background-repeat: no-repeat;
		border-radius: 6px;
		position: relative;
		// margin-bottom: 20px;

		.divbg-overlay {
			background: rgba(62, 250, 197, 0.7);
			position: absolute;
			height: 100%;
			width: 100%;
			left: 0;
			top: 0;
			color: #fff;
			font-size: 22px;
			text-align: center;
			opacity: 0;
			visibility: hidden;
			transition: all .5s ease;
		}
	}
	// &:hover .img .divbg-overlay, &.active .img .divbg-overlay {
	// 	opacity: 1;
	// 	visibility: visible;
	// }
	.img .divbg-overlay .location {
		position: absolute;
		top: 50%;
		transform: translateY(-50%);
		width: 100%;
		padding: 0 15px;
		img {
			margin-right: 10px;
		}
	}
}

.user-image-text {
	position: relative;
	padding-left: 90px;
	padding-top: 10px;
}

.div-bg {
	.usr-img {
		position: absolute;
		left: 0;
		top: 0;

		img {
			height: 50px;
			width: 50px;
			border-radius: 100%;
			border: 4px solid transparent;
			transition: all .5s ease;
		}
		.usr-name {
			// width: 80px;
			// text-overflow: ellipsis;
			// overflow: hidden;
			// white-space: nowrap;
			font-size: 15px;
			color: #979999;
			// text-align: center;
			// padding-top: 5px;
		}
	}
	// &:hover .usr-img img, &.active .usr-img img {
	// 	border: 4px solid $theme-green;
	// }
	// &:hover .usr-img .usr-name, &.active .usr-img .usr-name {
	// 	color: $theme-green;
	// }
	.usr-txt {
		font-size: 13px;
		ul {
			// padding-bottom: 10px;
		}
		li{
			margin-bottom: 5px;

			&.degree {
				color: $theme-green;
				position: relative;
				display: block;
				// padding-left: 26px;
				text-overflow: ellipsis;
				overflow: hidden;
				white-space: nowrap;
				margin-bottom: 0px;
			}
		}

		.two-links {
			display: flex;
			li {
				&:first-child {
					border-right: 1px solid #999;
					width: 46%;
				}
				&:last-child a {
					padding-left: 10px;
				}
			}
		}
		span {
			&.usr-location {
				color: #babbbb;
			}
			> img {
				// position: absolute;
				// left: 0;
			}
			&:hover {
				color: #979999;
			}
		}
		h6 {
			font-size: 14px;
			color: #babbbb;
			text-overflow: ellipsis;
			overflow: hidden;
			white-space: nowrap;
		}
	}
	.location-new{
		position: absolute;
		bottom: 0;
		background: rgba(0,0,0,.4);
		color: white;
		width: 100%;
		padding: 10px 25px;

		.specialization{
			font-size: 20px;
		}
	}
}

.review-comment {
	padding-top: 10px;
	font-size: 10px;
	color: #dadada;
	position: relative;
	.review-count {
		color: #f5ca33;
	}
	.comment {
		position: absolute;
		right: 0;
		top: 2px;
		color: #dadada;
		font-size: 18px;
		transition: all .5s ease;
	}
}

.div-bg {
	&:hover .review-comment .comment, &.active .review-comment .comment {
		color: $theme-green;
	}
	.amount {
		color: #dadada;
		font-size: 12px;
		padding-top: 15px;
		padding-bottom: 30px;
		> * {
			display: inline-block;
			vertical-align: middle;
		}
		.digit {
			color: $theme-green;
			font-size: 24px;
			margin: 0 5px;
		}
	}
}

.load-more {
	padding-top: 30px;
	.default-btn {
		min-width: 160px;
		border-radius: 20px;
	}
}

.without-img-bg {
	border: 1px solid #e6e6e6;
	background: #f4fcfa;
	color: #979999;
	border-radius: 6px;
	padding: 20px;
	transition: all .4s ease;

	h4 {
		color: $theme-green;
		font-size: 22px;
		text-overflow: ellipsis;
		overflow: hidden;
		white-space: nowrap;
		margin-bottom: 20px;
		transition: all .4s ease;
	}
}

.without-bg-con {
	&:hover .without-img-bg {
		background: $theme-green;
		color: #fff;
		cursor: pointer;
		h4 {
			color: #fff;
		}
	}
	&.active .without-img-bg h4 {
		color: #fff;
	}
}

.without-img-bg {
	p {
		margin-bottom: 0px;
		font-size: 13px;
	}
	.usr-txt p {
		margin-bottom: 10px;
	}
}

.without-img-bg .usr-txt .edu-cap {
	background: url(<%= asset_url "reskin/edu-cap.png" %>) no-repeat left;
	padding-left: 30px;
	color: #979999;
}

.without-img-bg .usr-txt .usr-location {
	background: url(<%= asset_url "reskin/location-small.png" %>) no-repeat left;
	padding-left: 30px;
	color: #979999;
}

.without-bg-con:hover .without-img-bg .usr-txt .edu-cap,
.without-bg-con.active .without-img-bg .usr-txt .edu-cap {
	color: #fff;
	// background: asset_url('reskin/edu-cap-white.png') no-repeat left;
	background: url(<%= asset_url "reskin/edu-cap-white.png" %>) no-repeat left;
}

.without-bg-con:hover .without-img-bg .usr-txt .usr-location,
.without-bg-con.active .without-img-bg .usr-txt .usr-location {
	color: #fff;
	// background: asset_url('reskin/location-small-white.png') no-repeat left;
	background: url(<%= asset_url "reskin/location-small-white.png" %>) no-repeat left;
}

.user-image-text.user-right {
	padding: 5px 100px 50px 0px;
}

.div-bg .user-image-text.user-right {
	.usr-img {
		left: auto;
		right: 0;
	}
	+ p {
		line-height: 26px;
	}
}

.without-img-bg .review-comment {
	padding-top: 30px;
}

.without-bg-con {
	&:hover .without-img-bg .review-comment, &.active .without-img-bg .review-comment, &:hover .without-img-bg .review-comment .review-count, &.active .without-img-bg .review-comment .review-count {
		color: #fff;
	}
}

.user-time {
	margin-top: 25px;
	padding-top: 10px;
	position: relative;
	padding-left: 80px;
	.usr-img {
		position: absolute;
		left: 0;
		top: 0;
		img {
			height: 60px;
